<div class="form-group">
  <label>{{localize "ABILITY_LABEL"}}:</label>
  <select id="ability-select">
    {{#each abilityChoices}}
      <option value="{{@key}}">{{this}}</option>
    {{/each}}
  </select>
</div>
<div class="form-group">
  <label>{{localize "PLAYERS_LABEL"}}:</label>
  <select id="player-select" multiple>
    {{#each players}}
      <option value="{{this.value}}">{{this.label}}</option>
    {{/each}}
  </select>
</div>